Cell[TextData[{
 "Plugging equation (",
 
 CounterBox["DisplayFormulaNumbered", "eq:relativeIntensity"],
 ") into Q(\[Tau]), we get"
}], "Text",
 CellChangeTimes->{{3.826309767999604*^9, 3.826309821160747*^9}, {
  3.826309865048222*^9, 
  3.826309899619563*^9}},ExpressionUUID->"4f02b361-779e-487e-811c-\
282882561d30"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Q", 
   RowBox[{"(", 
    RowBox[{"\[Tau]", ",", "T"}], ")"}]}], "=", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"m", "=", "1"}], "M"], 
      RowBox[{
       SubscriptBox["c", "m"], 
       RowBox[{"(", "T", ")"}], 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["\[ImaginaryI]\[CapitalDelta]", "m"]}], 
         "\[Tau]"}]]}]}], "\[RightBracketingBar]"}], "2"], 
   "."}]}]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.826309909033846*^9, 3.826309909655429*^9}, {
  3.826309948277525*^9, 3.826310046784136*^9}},
 TextAlignment->Center,
 CellTags->"eq:Qtau",ExpressionUUID->"5ed3cc7d-ffe0-4943-90f9-222aa34ae870"],

Cell[TextData[{
 "Upon expanding the square, the \[Tau] dependent part of Q(\[Tau],T) \
resolves to a sum of cosines with period 2\[Pi]/",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftBracketingBar]", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "m"], "-", 
     SubscriptBox["\[CapitalDelta]", 
      RowBox[{"m", "'"}]]}]}], TraditionalForm]],ExpressionUUID->
  "2858486c-c8af-4996-8ef7-1dbd483212dd"],
 "\[RightBracketingBar]. Calculating these periods, we get"
}], "Text",
 CellChangeTimes->{{3.826310030317325*^9, 3.82631007548352*^9}, 
   3.8263101166669903`*^9, {3.826310860881934*^9, 3.826310981427741*^9}, {
   3.826311033134234*^9, 3.826311102604384*^9}, {3.826311161938159*^9, 
   3.826311162039604*^9}, {3.826311267286458*^9, 
   3.826311270084454*^9}},ExpressionUUID->"16eedb8b-a371-42b6-8b07-\
9ab77389f556"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Sort", " ", "the", " ", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "m"], "'"}], "s", " ", "from", " ", 
    "smallest", " ", "to", " ", "largest", " ", "first", " ", "so", " ", 
    RowBox[{"it", "'"}], "s", " ", "easier", " ", "to", " ", "associate", " ",
     "the", " ", "line", " ", "differences", " ", "with", " ", "the", " ", 
    "spectrum"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"sortedLine\[CapitalDelta]", " ", "=", " ", 
     RowBox[{"Sort", "[", "line\[CapitalDelta]", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"line\[CapitalDelta]\[CapitalDelta]", " ", "=", " ", 
    RowBox[{"UnitConvert", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"Reap", "[", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", " ", 
           RowBox[{"i", " ", "<", " ", 
            RowBox[{"Length", "[", "line\[CapitalDelta]", "]"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", "=", 
              RowBox[{"i", "+", "1"}]}], ",", 
             RowBox[{"j", "\[LessEqual]", " ", 
              RowBox[{"Length", "[", "line\[CapitalDelta]", "]"}]}], ",", 
             RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"Sow", "[", 
               RowBox[{"2", 
                RowBox[{"\[Pi]", "/", 
                 RowBox[{"Abs", "[", 
                  RowBox[{
                   RowBox[{"sortedLine\[CapitalDelta]", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "-", 
                   RowBox[{"sortedLine\[CapitalDelta]", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}]}], "]"}], 
              ";"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
          "]"}], "\[IndentingNewLine]", "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}], ",", "\[IndentingNewLine]", 
      "\"\<Picoseconds\>\""}], "\[IndentingNewLine]", "]"}]}], 
   "\[IndentingNewLine]"}]}]], "Input",
 CellChangeTimes->{{3.826310239880116*^9, 3.826310287823065*^9}, {
