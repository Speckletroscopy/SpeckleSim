Cell["We can finally plot our signal to see what it looks like!", "Text",
 CellChangeTimes->{{3.826311525688547*^9, 
  3.8263115483961267`*^9}},ExpressionUUID->"4b344b65-87ba-47a9-9322-\
cf20f76f47af"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"sigQ", "[", 
    RowBox[{"\[Tau]_", ",", "T_"}], "]"}], ":=", 
   RowBox[{"Re", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Total", "[", 
      RowBox[{
       RowBox[{"relativeIntensity", "[", "T", "]"}], "*", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], "*", "line\[CapitalDelta]THz", "*", 
         "\[Tau]"}], "]"}]}], "]"}], "\[IndentingNewLine]", "*", 
     RowBox[{"Conjugate", "[", 
      RowBox[{"Total", "[", 
       RowBox[{
        RowBox[{"relativeIntensity", "[", "T", "]"}], "*", 
