Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"sigQ", "[", 
    RowBox[{"\[Tau]_", ",", "T_"}], "]"}], ":=", 
   RowBox[{"Re", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Total", "[", 
      RowBox[{
       RowBox[{"relativeIntensity", "[", "T", "]"}], "*", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], "*", "line\[CapitalDelta]THz", "*", 
         "\[Tau]"}], "]"}]}], "]"}], "\[IndentingNewLine]", "*", 
     RowBox[{"Conjugate", "[", 
      RowBox[{"Total", "[", 
       RowBox[{
        RowBox[{"relativeIntensity", "[", "T", "]"}], "*", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], "*", "line\[CapitalDelta]THz", "*", 
          "\[Tau]"}], "]"}]}], "]"}], "]"}]}], "\[IndentingNewLine]", "]"}]}],
   "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sigQUnits", "[", 
   RowBox[{"\[Tau]_", ",", "T_"}], "]"}], ":=", 
  RowBox[{"Re", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Total", "[", 
     RowBox[{
      RowBox[{"relativeIntensityUnits", "[", "T", "]"}], "*", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], "*", "line\[CapitalDelta]", "*", 
        "\[Tau]"}], "]"}]}], "]"}], "\[IndentingNewLine]", "*", 
    RowBox[{"Conjugate", "[", 
     RowBox[{"Total", "[", 
      RowBox[{
       RowBox[{"relativeIntensityUnits", "[", "T", "]"}], "*", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], "*", "line\[CapitalDelta]", "*", 
         "\[Tau]"}], "]"}]}], "]"}], "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
